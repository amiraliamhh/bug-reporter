{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["rext.js"],
          "css": ["style.css"]
        }
    ],
    "options_page": "options.html",
    "permissions": [
      "storage"
    ]
}